@model Vidly.Models.RandomMovieViewModel
@{
    ViewBag.Title = "Random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="popular">@Model.Movie.Name</h2>
@if (Model.Coustomers.Count == 0)
{
    <text>There is no coustomer</text>
}
else
{
    foreach (var coustomer in Model.Coustomers)
     {
         <li>@coustomer.Name</li>
    
     }
}



